{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "indent": ["warn", 2],
      "react/display-name": false
      // "react/jsx-uses-react": 1
    },
    "parserOptions": {
      "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "spread" : true,
            "restParams" : true
        }
    }
  }